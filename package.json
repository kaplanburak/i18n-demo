{
  "name": "sacre-client",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "npm run i18n && next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint './{src,pages,server}/**/*' --ext .tsx,.ts",
    "extract:i18n": "formatjs extract 'src/{pages,components,sections}/**/*.{ts,tsx}' --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/en.json",
    "compile:i18n": "formatjs compile-folder --ast content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/styled-system": "^5.1.10",
    "antd": "^4.14.0",
    "axios": "^0.21.1",
    "less": "^4.1.1",
    "next": "latest",
    "next-plugin-antd-less": "^0.1.4",
    "next-redux-wrapper": "^6.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.12.0",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-intl": "^5.17.7",
    "react-redux": "^7.2.2",
    "styled-components": "^5.2.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.15",
    "@types/node": "^12.20.6",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.11",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-formatjs": "^10.2.13",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-formatjs": "^2.15.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^1.0.6",
    "typescript": "^4.0.7"
  },
  "license": "MIT"
}
